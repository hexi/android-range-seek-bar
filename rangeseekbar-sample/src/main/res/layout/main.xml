<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:rsb="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <com.yahoo.mobile.client.android.util.rangeseekbar.RangeSeekBar
        android:id="@+id/sb_setter_0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        rsb:absoluteMinValue="1"
        rsb:absoluteMaxValue="250"
        rsb:showTextLabel="true"
        rsb:singleThumb="true"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8329f"/>
    <com.yahoo.mobile.client.android.util.rangeseekbar.RangeSeekBar
        android:id="@+id/sb_setter_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        rsb:absoluteMinValue="1"
        rsb:absoluteMaxValue="250"
        rsb:showTextLabel="false"
        rsb:singleThumb="true"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8329f"/>
    <com.yahoo.mobile.client.android.util.rangeseekbar.RangeSeekBar
        android:id="@+id/sb_setter_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        rsb:absoluteMinValue="1"
        rsb:absoluteMaxValue="250"
        rsb:showTextLabel="true"
        rsb:showDemoLabel="true"
        rsb:singleThumb="true"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8329f"/>
    <com.yahoo.mobile.client.android.util.rangeseekbar.RangeSeekBar
        android:id="@+id/sb_setter_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        rsb:absoluteMinValue="100"
        rsb:absoluteMaxValue="250"
        rsb:showTextLabel="true"
        rsb:showDemoLabel="false"
        rsb:singleThumb="false"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8329f"/>
</LinearLayout>